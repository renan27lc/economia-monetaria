dm_xphyp_rw_M72<-dm.test(PE_xphyp[,72], PE_rw[,72], alternative = "less",h = 1)
dm_xphyp_rw_M96<-dm.test(PE_xphyp[,96], PE_rw[,96], alternative = "less",h = 1)

