     coef(lm_A14)[7:11,48],
     coef(lm_A14)[7:11,72],
     coef(lm_A14)[7:11,96])

Lambda_hat<-as.data.frame(crossprod(solve(crossprod(t(beta_hat_M296))),crossprod(t(beta_hat_M296),t(c_hat_M296))))	# 5x5

#============== lambda_hat ============

vec_Sigma<-as.matrix(as.vector(Sigma_hat))	#25x1
B_ast_vecSigma<-crossprod(B_ast,vec_Sigma)		#120x1
Bast_M296_vecSigma<-crossprod(B_ast_M296,vec_Sigma)		#8x1

betabeta_inv<- solve(crossprod(t(beta_hat)))	#5x5
i_120<-as.matrix(rep(1, 120))     #120x1

a_hat<-as.matrix(coef(lm_A14)[1,])				#1x120

a_hat_M296<-cbind(				#1x8
	coef(lm_A14)[1,2],
	coef(lm_A14)[1,3],
	coef(lm_A14)[1,9],
	coef(lm_A14)[1,12],
	coef(lm_A14)[1,24],
	coef(lm_A14)[1,48],
	coef(lm_A14)[1,72],
	coef(lm_A14)[1,96])

i_8<-as.data.frame(rep(1,8))			#8x1

lambda_hat<-crossprod(				# 5x1
	betabeta_inv,
	crossprod(
		t(beta_hat),
		as.matrix(
			a_hat + 		#120x1
			(B_ast_vecSigma + 	#120x1	
			sig2_hat*i_120)/2)))	#120x1

#lambda_hat_M296<-crossprod(betabeta_inv,crossprod(t(beta_hat_M296),as.matrix(t(a_hat_M296)+(BascM296_vecSigma+sig2_hat*i_8)/2)))

U_hat<-as.matrix(lm_yields_pc5$residuals)	#199 x 121

#============== delta_hat ============

lm_rf_pc<-lm(yields[,1] ~ as.matrix(pc_scores))

delta_hat<-as.matrix(coef(lm_rf_pc))[1]       # 1x1
Delta_hat<-as.matrix(coef(lm_rf_pc))[-1]      # 120 x 1
Delta_hat_5pc<-as.matrix(coef(lm_rf_pc))[2:6] # 5x1

#============== MSPE adrian ============== 

i_5<-as.matrix(rep(1, 5))     													#5x1

kappa_hat <-t(crossprod(as.matrix(beta_hat),as.matrix(lambda_hat)) -(B_ast_vecSigma + sig2_hat*i_120)/2) # -t(delta_hat*i_120)    #1x120

Kappa_hat <-crossprod(as.matrix(beta_hat),as.matrix(Lambda_hat)) # +t(crossprod(t(Delta_hat_5pc),t(i_120)))   #120x5

csi_hat <- t(crossprod(as.matrix(beta_hat),t(V_hat))) + U_hat_fin - U_hat_ini

E_rxs_adrian <- t(crossprod(kappa_hat,t(i_198))) + crossprod(t(X_),t(Kappa_hat)) + U_hat_ini # + csi_hat      #198x120

