#============== Lambda_hat ============

c_hat<-as.matrix(coef(lm_A14)[7:11,])

c_hat_M296<-cbind(
     coef(lm_A14)[7:11,2],
     coef(lm_A14)[7:11,3],
     coef(lm_A14)[7:11,9],
     coef(lm_A14)[7:11,12],
